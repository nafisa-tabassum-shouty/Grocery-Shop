<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".sidePage">



    <ImageView
        android:id="@+id/backgr"
        android:layout_width="200dp"
        android:layout_height="match_parent"
        android:background="@drawable/sidepage"/>


    <ImageView
        android:id="@+id/cancel"
        android:layout_width="100dp"
        android:layout_height="70dp"
        android:layout_marginTop="10dp"
        app:srcCompat="@drawable/cancel"
        />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="86dp"
        android:layout_height="130dp"
        android:layout_alignStart="@+id/aboutus"
        android:layout_marginStart="-2dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="150dp"
        app:srcCompat="@drawable/user" />
    <TextView
        android:id="@+id/textView29"
        android:layout_marginTop="200dp"
        android:textSize="20dp"
        android:layout_marginLeft="170dp"
        android:textColor="@color/black"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="User Name :" />
    <TextView
        android:id="@+id/username"
        android:layout_marginTop="200dp"
        android:textSize="20dp"
        android:layout_marginLeft="5dp"
        android:layout_toRightOf="@id/textView29"
        android:textColor="@color/black"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="NAFISA" />
    <TextView
        android:id="@+id/textView30"
        android:layout_below="@id/textView29"
        android:layout_marginTop="30dp"
        android:textSize="20dp"
        android:layout_marginLeft="170dp"
        android:textColor="@color/black"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Email Address :" />
    <TextView
        android:id="@+id/EMAIL"
        android:layout_below="@id/textView30"
        android:textSize="20dp"
        android:layout_marginLeft="170dp"
        android:layout_marginTop="30dp"
        android:textColor="@color/black"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="" />

    <TextView
        android:id="@+id/home"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/aboutus"
        android:layout_marginStart="-6dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="310dp"
        android:text="Home"
        android:textColor="@color/white"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/aboutus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/home"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="43dp"
        android:text="About Us"
        android:textColor="@color/white"
        android:textSize="20dp" />

    <TextView
        android:layout_alignStart="@+id/aboutus"
        android:id="@+id/cart"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/white"
        android:layout_marginTop="35dp"
        android:layout_marginLeft="4dp"
        android:layout_below="@id/aboutus"
        android:textSize="20dp"
        android:text="Cart" />

    <TextView
        android:layout_alignStart="@+id/aboutus"
        android:id="@+id/catagory"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/cart"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="33dp"
        android:text="Categories"
        android:textColor="@color/white"
        android:textSize="20dp" />

    <TextView
        android:layout_alignStart="@+id/aboutus"
        android:id="@+id/logout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/catagory"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="38dp"
        android:text="LogOut"
        android:textColor="@color/white"
        android:textSize="20dp" />
</RelativeLayout>